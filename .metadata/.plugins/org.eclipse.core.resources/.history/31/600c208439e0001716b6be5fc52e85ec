public class FizzBuzz
{
	public static void main(String[] args)
	{
		for (int n = 1 ; n <= 100 ; n = n+1)
		{
			System.out.println(n);
			if ((n%3)==0 && (n%5)!=0)
			{
				System.out.println("fizz");
			}
			if ((n%5)==0 && (n%3)!=0)
			{
				System.out.println("buzz");
			}
			if ((n%3)==0 && (n%5)==0)
			{
				System.out.println("fizzbuzz");
			}
		}
	}
}
