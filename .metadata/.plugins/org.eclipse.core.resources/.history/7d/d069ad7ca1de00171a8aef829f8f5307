import java.util.Random;

public class DoubleDice 
{
	public static void main ( String[] args )
	{
		Random r = new Random();
		int rnum = 1 + r.nextInt(6);
		int rnum1= 1 + r.nextInt(6);
		int num=rnum+rnum1;
		System.out.println("HERE COMES THE DICE!");
		System.out.println("Roll #1: "+rnum);
		System.out.println("Roll #2: "+rnum1);
		System.out.println("The total is  "+(num+"!"));
		if (num < 10)
		{
			int rnum2 = 1 + r.nextInt(6);
			int rnum3= 1 + r.nextInt(6);
			int num1=rnum2+rnum3;
			System.out.println("HERE COMES THE DICE!");
			System.out.println("Roll #1: "+rnum2);
			System.out.println("Roll #2: "+rnum3);
			System.out.println("The total is  "+(num1+"!"));
		}
		
		
	}
}
