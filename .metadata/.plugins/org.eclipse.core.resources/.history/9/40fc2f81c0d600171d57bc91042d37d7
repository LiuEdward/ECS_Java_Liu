public class MoreVariableAndPrinting 
{
	public static void main(String[] args)
	{
		String myName, myEyes, myTeeth, myHair;
		int myAge, myHeight, myWeight;
		
		myName="Zed A. Shaw";
		myAge=35;
		myHeight=74;
		myWeight=180;
		myEyes="Blue";
		myTeeth="White";
		myHair="Brown";
		
		System.out.println("Let's talk about " myName+".")
	}
}
