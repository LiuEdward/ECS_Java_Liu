import java.util.Scanner;
public class CountingMachine 
{
	public static void main(String[] args)
	{
		Scanner keyboard = new Scanner(System.in);
		int x;
		System.out.println("Count to:");
		x = keyboard.nextInt();
		for (int n = 0 ; n <= x ; n = n++)
		{
			System.out.println(n);
		}
		
		
		
		
		
		
		
		
	}
}
